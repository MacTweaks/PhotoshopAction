\b0 \cf0 \
1. Install Xcode\'a0\
\
2. Install Command Line Tools\'a0\
\
\pard\pardeftab720\sl260

\f1\fs16 \cf0 \cb2 xcode-select --install\
\pard\pardeftab720\sl260

\f0\fs18 \cf0 \cb1 \
3. Install {\field{\*\fldinst{HYPERLINK "http://brew.sh/"}}{\fldrslt http://brew.sh/}}\
\
\pard\pardeftab720\sl260

\f1\fs16 \cf0 \cb2 /usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"\
\pard\pardeftab720\sl260

\f0\fs18 \cf0 \cb1 \
